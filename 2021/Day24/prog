xadd = [10, 15, 14, 15, -8, 10, -16, -4, 11, -3, 12, -7, -15, -7]
yadd = [2, 16, 9, 0, 1, 12, 6, 6, 3, 5, 9, 3, 2, 3]
zdiv = [1, 1, 1, 1, 26, 1, 26, 26, 1, 26, 1, 26, 26, 26]

x, z = 0, 0
for i in range(14):
    x = (z % 26) + xadd[i]
    z //= zdiv[i]
    if x != inp[i]:
        z *= 26
        z += inp[i] + yadd[i]